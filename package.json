{
  "name": "randomnames",
  "version": "1.0.0",
  "description": "A project that generates random names from wiktionary data",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "src/Names.js",
  "scripts": {
    "start": "node src/Names.js",
    "dev": "nodemon src/Names.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint .",
    "generate:ssn": "node src/ssn.js",
    "generate:bulk": "node src/ssndb.js",
    "generate:continuous": "while true; do node src/Names.js; sleep 1; done"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "csvtojson": "^2.0.10",
    "fodselsnummer-generator": "^1.0.0",
    "math": "^0.0.3",
    "moment": "^2.30.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devicemanager/randomNames.git"
  },
  "keywords": [
    "java",
    "shell",
    "bash",
    "sql",
    "js",
    "javascript",
    "ssn",
    "norwegian",
    "names",
    "female",
    "male",
    "surname",
    "sex",
    "wiktionary",
    "sqlite",
    "sqlite3",
    "node",
    "name",
    "picker"
  ],
  "author": "Ren√© Geers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devicemanager/randomNames/issues"
  },
  "homepage": "https://github.com/devicemanager/randomNames#readme"
}
